<resources xmlns:android="http://schemas.android.com/apk/res/android">

	<!--
		Base application theme, dependent on API level. This theme is replaced
		by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
	-->
	<style name="AppBaseTheme" parent="android:Theme.Light">
		<!--
			Theme customizations available in newer API levels can go in
			res/values-vXX/styles.xml, while customizations related to
			backward-compatibility can go here.
		-->
	</style>

	<!-- Application theme. -->
	<style name="AppTheme" parent="AppBaseTheme">
		<!-- All customizations that are NOT specific to a particular API-level can go here. -->
		<item name="android:soundEffectsEnabled">false</item>
	</style>

	<style name="Theme_Dialog" parent="android:Theme.Light">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowBackground">@android:color/transparent</item>
	</style>

	<style name="MyTheme" parent="android:Theme.Holo.Light">
		<item name="android:windowActionBar">false</item>
		<item name="android:windowNoTitle">true</item>
	</style>

	<style name="settings_panel_black_general">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:orientation">vertical</item>
	</style>
	<style name="settings_panel_black" parent="settings_panel_black_general">
	</style>
	<style name="settings_title_general">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">@android:color/white</item>
	</style>
	<style name="settings_title" parent="settings_title_general">
		<item name="android:layout_gravity">end|top</item>
		<item name="android:layout_marginRight">62dp</item>
		<item name="android:layout_marginTop">7dp</item>
		<item name="android:textSize">@dimen/dimension_20</item>
	</style>
	<style name="button_back_general">
		<item name="android:layout_gravity">start</item>
		<item name="android:text">@string/back</item>
		<item name="android:minHeight">45dp</item>
		<item name="android:minWidth">45dp</item>
	</style>
	<style name="button_back" parent="button_back_general">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
	</style>
	<style name="settings_field_title_general">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">@color/white</item>
	</style>
	<style name="settings_field_title" parent="settings_field_title_general">
		<item name="android:textSize">@dimen/dimension_25</item>
	</style>
	<style name="settings_field_value_general">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textColor">@color/white</item>
	</style>
	<style name="settings_field_value" parent="settings_field_value_general">
		<item name="android:textSize">@dimen/dimension_12</item>
	</style>
	<style name="settings_bottom_bar_general">
	</style>
	<style name="settings_bottom_bar" parent="settings_bottom_bar_general">
		<item name="android:layout_height">wrap_content</item>
	</style>
	<style name="settings_button_disconnect_general">
	</style>
	<style name="settings_button_disconnect" parent="settings_button_disconnect_general">
		<item name="android:layout_height">45dp</item>
		<item name="android:layout_gravity">center_vertical</item>
		<item name="android:textSize">@dimen/dimension_12</item>
	</style>

